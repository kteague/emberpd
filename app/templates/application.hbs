<section id="todoapp">
  <header id="header">
    <h1>Passport Date</h1>
    <input type="text" id="new-todo" placeholder="Who do you want to love?" />
  </header>

  <section id="main">
    <ul id="todo-list">
      <li>
		<input type="checkbox" class="toggle">
        <label>{{#link-to "about"}}About{{/link-to}}</label>
      </li>
      <li>
        <input type="checkbox" class="toggle">
        <label><a href="./profile/1">First Profile</a></label><button class="destroy"></button>
      </li>
	  
	  {{#each profile as |item|}}
	    <li>
	      <input type="checkbox" class="toggle">
	      <label>{{item.firstName}}</label><button class="destroy"></button>
	    </li>
	  {{/each}}
    
	  <li>
	    <input type="checkbox" class="toggle">
	  	<label>{{outlet}}</label>
	  </li>
	
	</ul>

	
    <input type="checkbox" id="toggle-all">
  </section>

  <footer id="footer">
    <ul id="filters">
      <li>
        <a href="all" class="selected">All</a>
      </li>
      <li>
        <a href="active">Search</a>
      </li>
    </ul>

    <button id="clear-completed">
      Clear completed (1)
    </button>
  </footer>
</section>

<footer id="info">
  <p>&copy; 2015 Passport Date Media Inc.</p>
</footer>
